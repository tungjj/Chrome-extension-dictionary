{
    
   
  "manifest_version": 2,
  "name": "My Extension",
  "version": "0.1",
  "description": "Look up the definition",
  "content_scripts":[
    {
      "js": ["content.js"],
      "matches":["<all_urls>"]
    }
  ],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action":{
    "default_icon": "/images/get_started32.png",
    "default_popup": "popup.html",
    "default_title": "Dictionary for me:"
  },
  "permissions":["tabs"]
  
  }

  